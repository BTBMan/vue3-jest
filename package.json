{
  "name": "vue3-jest",
  "version": "0.0.0",
  "scripts": {
    "serve": "npm run dev",
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "preview:build": "npm run build && npm run preview",
    "type:check": "vue-tsc --noEmit --skipLibCheck",
    "prepare": "husky install",
    "lint:eslint": "eslint \"**/*.{vue,ts,tsx,js,jsx}\"",
    "lint:prettier": "prettier --write --no-editorconfig --config-precedence file-override \"**/*.{js,ts,json,jsonc,jsx,tsx,css,less,scss,vue,html,md}\"",
    "lint:lint-staged": "lint-staged",
    "fix:eslint": "npm run lint:eslint -- --fix",
    "fix:prettier": "npm run lint:prettier -- --fix",
    "commit": "node ./node_modules/cz-customizable/standalone.js",
    "test:unit": "jest"
  },
  "dependencies": {
    "vue": "^3.1.5"
  },
  "devDependencies": {
    "@babel/core": "^7.14.8",
    "@babel/preset-env": "^7.14.8",
    "@commitlint/cli": "^9.0.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@types/jest": "^26.0.24",
    "@types/node": "^16.4.1",
    "@typescript-eslint/eslint-plugin": "^4.28.4",
    "@typescript-eslint/parser": "^4.28.4",
    "@vitejs/plugin-vue": "^1.2.5",
    "@vitejs/plugin-vue-jsx": "^1.1.6",
    "@vue/babel-plugin-jsx": "^1.0.6",
    "@vue/compiler-sfc": "^3.1.5",
    "@vue/test-utils": "^2.0.0-rc.12",
    "babel-jest": "^26.6.3",
    "commitlint-config-cz": "^0.13.2",
    "cz-customizable": "^6.3.0",
    "eslint": "^7.31.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-define-config": "^1.0.9",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jest": "^24.4.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-vue": "^7.14.0",
    "husky": "^7.0.1",
    "jest": "^26.6.3",
    "lint-staged": "^11.1.1",
    "prettier": "^2.3.2",
    "ts-jest": "^26.5.6",
    "typescript": "4.2.4",
    "vite": "^2.4.3",
    "vite-plugin-eslint": "^1.3.0",
    "vue-eslint-parser": "^7.9.0",
    "vue-jest": "^5.0.0-alpha.10",
    "vue-tsc": "^0.2.2"
  }
}
